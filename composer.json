{
    "name": "braveware/flarum-theme-illuminated",
    "description": "Illuminated Theme for Flarum",
    "license": "GPL-3.0-or-later",
    "type": "flarum-extension",
    "require": {
        "flarum/core": ">=0.1.0-beta.16 <=0.1.0"
    },
    "autoload": {
        "psr-4": {
            "Illuminated\\Theme\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Illuminated Theme",
            "icon": {
                "name": "far fa-lightbulb",
                "backgroundColor": "#2A2C46",
                "color": "#DAA33B"
            }
        }
    }
}
